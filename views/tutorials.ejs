<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<title>jsHarmony Tutorials - <%= title %></title>
<link href='https://fonts.googleapis.com/css?family=Droid+Sans|Material+Icons|PT+Sans' rel='stylesheet' type='text/css' />
<link rel="stylesheet" type="text/css" href="/application.css" />
<script type="text/javascript" src="/js/jsHarmony.js"></script>
<script type="text/javascript">
var <%-req.jshsite.instance%> = new jsHarmony(<%-req.getJSClientParams()%>);
</script>
<script type="text/javascript" src="/js/site.js"></script>
<script type="text/javascript" src="/application.js"></script>
<script type="text/javascript" src="/js/tutorials.js"></script>
<script type="text/javascript" src="/js/highlight/highlight.pack.js"></script>
<link rel="stylesheet" href="/js/highlight/styles/vs.css" />
<script type="text/javascript">
hljs.initHighlightingOnLoad();
<%=req.jshsite.instance%>.App.tutorials = new jsHarmonyTutorials(<%=req.jshsite.instance%>);
<%=req.jshsite.instance%>.App.tutorials.Init({
  tutorials: <%-JSON.stringify(params.tutorials)%>,
  tutorialIDs: <%-JSON.stringify(params.tutids)%>,
  tutorialsMenu: <%-JSON.stringify(params.menu)%>,
  tutorialsLOV: []
});
</script>
</head>

<body class="tutorials">
<div class="xmain">

<div class="xhead">
  <a class="xbrand_link" href="<%-ejsext.iif(req.isAuthenticated,'',req.jshsite.home_url)%>/">&nbsp;</a>
  <div class="xlogo">
    <a class="xlogoimg" href="<%-ejsext.iif(req.isAuthenticated,'',req.jshsite.home_url)%>/"><img src="/images/tutorials-logo.png" alt="jsHarmony Tutorials" title="jsHarmony Tutorials" /></a>
    <a class="xsublogo" href="<%-ejsext.iif(req.isAuthenticated,'',req.jshsite.home_url)%>/" alt="Tutorials Collection" title="Tutorials Collection"><img src="/images/tutorials-tagline.png" /></a>
  </div>
  <div class="tutorials_search">
    <form action="/_search" method="get" onSubmit="<%=req.jshsite.instance%>.App.tutorials.searchTutorials(<%=req.jshsite.instance%>.$root('.tutorials_search .query').val()); return false;">
      <input name="query" class="query" type="text" value="Search" /><input class="submit" type="submit" />
    </form>
  </div>
</div>

<div class="tutorials_menu xform_ctrl tree" data-onselected="<%=req.jshsite.instance%>.App.tutorials.onTutorialSelected(nodeid, this);"></div>

<div class="tutorials_body">
<%- body %>
</div><!-- tutorials_body -->

</div><!--xmain-->
</body>
</html>