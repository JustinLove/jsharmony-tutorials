<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<title><%=global.site_title%> - <%= title %></title>
<link href='https://fonts.googleapis.com/css?family=Droid+Sans|Material+Icons|PT+Sans' rel='stylesheet' type='text/css' />
<link rel="stylesheet" type="text/css" href="/system.css" />
<script type="text/javascript"><%-req.getBaseJS()%></script>
<script type="text/javascript" src="/js/main.js"></script>
<% if(req.isAuthenticated){ %><script type="text/javascript" src="/js/browser.update.js"></script><% } %>
<script type="text/javascript" src="/js/site.js"></script>
<script type="text/javascript" src="/system.js"></script>
<script type="text/javascript" src="/jquery-ui/js/jquery-ui-1.10.3.custom-aspa.min.js"></script>
<link rel="stylesheet" href="/jquery-ui/css/jquery-ui-1.10.3.custom.min.css" />
<script type="text/javascript" src="/js/jquery.colorbox-min.js"></script>
<link rel="stylesheet" href="/js/colorbox/colorbox.css" />
<script type="text/javascript" src="/js/tutorials.js"></script>
<script type="text/javascript" src="/js/highlight/highlight.pack.js"></script>
<link rel="stylesheet" href="/js/highlight/styles/vs.css" />
<script>hljs.initHighlightingOnLoad();</script>
</head>

<body class="tutorials">
<div id="xmain">

<div id="xhead">
  <a id="xbrand_link" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/">&nbsp;</a>
  <div id="xlogo">
    <a id="xlogoimg" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/"><img src="/images/tutorials-logo.png" alt="<%=global.site_title%>" title="<%=global.site_title%>" /></a>
    <a id="xsublogo" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/" alt="Tutorials Collection" title="Tutorials Collection"><img src="/images/tutorials-tagline.png" /></a>
  </div>
  <div id="tutorials_search">
    <form action="/_search" method="get" onSubmit="searchTutorials($('#tutorials_search .query').val()); return false;">
      <input name="query" class="query" type="text" value="Search" /><input class="submit" type="submit" />
    </form>
  </div>
</div>

<script type="text/javascript">
var tutorials = <%-JSON.stringify(params.tutorials)%>;
var tutorialsMenu = <%-JSON.stringify(params.menu)%>;
var tutorialsLOV = [];
var jsh_frontsalt = <%-JSON.stringify(global.frontsalt)%>;
var jsh_popups = <%-JSON.stringify(params.popups)%>;
</script>

<div id="tutorials_menu" class="xform_ctrl tree" data-onselected="onTutorialSelected(nodeid, this);"></div>

<div id="tutorials_body">
<%- body %>
</div><!-- tutorials_body -->

</div><!--xmain-->
</body>
</html>